{
  "$schema": "https://ui.shadcn.com/schema/registry-item.json",
  "name": "avatar-stack-demo",
  "title": "Avatar Stack Demo",
  "registryDependencies": [
    "avatar",
    "@shadcraft/avatar-stack"
  ],
  "files": [
    {
      "path": "examples/avatar-stack-demo.tsx",
      "content": "import { User } from \"lucide-react\";\n\nimport { Avatar, AvatarFallback, AvatarImage } from \"@/components/ui/avatar\";\nimport { AvatarStack } from \"@/components/ui/avatar-stack\";\n\nexport function AvatarStackDemo() {\n  return (\n    <div className=\"flex flex-col items-center gap-12 p-5 lg:p-8\">\n      <div className=\"flex flex-col items-center gap-2\">\n        <span className=\"text-sm font-medium text-balance text-muted-foreground\">Default</span>\n        <AvatarStack>\n          {avatarsSrc.map((src) => (\n            <Avatar key={src}>\n              <AvatarImage src={src} />\n              <AvatarFallback>\n                <User />\n              </AvatarFallback>\n            </Avatar>\n          ))}\n        </AvatarStack>\n      </div>\n\n      <div className=\"flex flex-col items-center gap-2\">\n        <span className=\"text-sm font-medium text-balance text-muted-foreground\">\n          Max number of avatars\n        </span>\n        <AvatarStack max={3}>\n          {avatarsSrc.map((src) => (\n            <Avatar key={src}>\n              <AvatarImage src={src} />\n              <AvatarFallback>\n                <User />\n              </AvatarFallback>\n            </Avatar>\n          ))}\n        </AvatarStack>\n      </div>\n\n      <div className=\"flex flex-col items-center gap-2\">\n        <span className=\"text-sm font-medium text-balance text-muted-foreground\">Unmasked</span>\n        <AvatarStack max={4} mask={false}>\n          {avatarsSrc.map((src) => (\n            <Avatar key={src}>\n              <AvatarImage src={src} />\n              <AvatarFallback>\n                <User />\n              </AvatarFallback>\n            </Avatar>\n          ))}\n        </AvatarStack>\n      </div>\n\n      <div className=\"flex flex-col items-center gap-2\">\n        <span className=\"text-sm font-medium text-balance text-muted-foreground\">\n          Overlap Ratio 0.3 (30% of avatar size)\n        </span>\n        <AvatarStack overlapRatio={0.3}>\n          {avatarsSrc.map((src) => (\n            <Avatar key={src}>\n              <AvatarImage src={src} />\n              <AvatarFallback>\n                <User />\n              </AvatarFallback>\n            </Avatar>\n          ))}\n        </AvatarStack>\n      </div>\n\n      <div className=\"flex flex-col items-center gap-2\">\n        <span className=\"text-sm font-medium text-balance text-muted-foreground\">\n          Vertical orientation\n        </span>\n        <AvatarStack orientation=\"vertical\" max={3}>\n          {avatarsSrc.map((src) => (\n            <Avatar key={src}>\n              <AvatarImage src={src} />\n              <AvatarFallback>\n                <User />\n              </AvatarFallback>\n            </Avatar>\n          ))}\n        </AvatarStack>\n      </div>\n    </div>\n  );\n}\n\nconst avatarsSrc = [\n  \"https://shadcraft-free.vercel.app/assets/avatars/person-1.webp\",\n  \"https://shadcraft-free.vercel.app/assets/avatars/person-2.webp\",\n  \"https://shadcraft-free.vercel.app/assets/avatars/person-3.webp\",\n  \"https://shadcraft-free.vercel.app/assets/avatars/person-4.webp\",\n  \"https://shadcraft-free.vercel.app/assets/avatars/person-5.webp\",\n];\n",
      "type": "registry:example"
    }
  ],
  "meta": {
    "bundle": "pro-marketing"
  },
  "type": "registry:example"
}